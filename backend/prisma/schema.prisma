generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "postgresql"
  url      = env("TURSO_DATABASE_URL")
}

model Event {
  eventID String @id @default(uuid())

  actor String

  eventType String

  ocurredAt DateTime @default(now())
}
